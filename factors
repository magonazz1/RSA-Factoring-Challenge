#!/bin/bash

file="$1"

if [ -z "$file" ]; then
  echo "Usage: $0 <file>"
  exit 1
fi

if [ ! -f "$file" ]; then
  echo "File '$file' not found."
  exit 1
fi

while read -r number; do
  if [[ $number -le 1 ]]; then
    echo "Skipping $number as it's not a natural number greater than 1."
    continue
  fi

  factors=""
  for ((i = 2; i <= number; i++)); do
    while ((number % i == 0)); do
      factors="$factors $i"
      number=$((number / i))
    done
  done

  if [[ -n "$factors" ]]; then
    factors=($factors)
    if (( ${#factors[@]} == 2 )); then
      echo "$number=${factors[0]}*${factors[1]}"
    else
      echo "Unable to factorize $number."
    fi
  fi
done < "$file"
